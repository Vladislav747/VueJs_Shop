{
  "name": "vuejs_shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "debug": "cd ./server && node --nolazy --inspect-brk=9229 server.js",
    "server_and_client": "PORT=8080 npm run start & PORT=8081 npm run serve",
    "start": "cd ./server && node server.js",
    "start_sqlite": "cd ./server && $typeDB=sqlite node server.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "app-root-path": "^2.2.1",
    "axios": "^0.18.1",
    "cors": "^2.8.5",
    "epilogue": "^0.7.1",
    "express": "^4.17.1",
    "express-cache-controller": "^1.1.0",
    "firebase-admin": "^8.10.0",
    "firebase-tools": "^8.0.1",
    "html": "^1.0.0",
    "moment": "^2.24.0",
    "mongoose": "^5.6.3",
    "node-sass": "^4.12.0",
    "noty": "^3.2.0-beta",
    "npm": "^6.14.4",
    "path": "^0.12.7",
    "pug": "^2.0.4",
    "sass-loader": "^7.1.0",
    "semantic-ui-css": "^2.4.1",
    "sequelize": "^4.44.0",
    "sqlite3": "^4.0.9",
    "uuid": "^3.3.2",
    "vue": "^2.6.10",
    "vue-input-tag": "^2.0.6",
    "vue-router": "^3.0.6",
    "vue-slider-component": "^3.1.1",
    "vue2-datepicker": "^2.12.0",
    "vuelidate": "^0.7.4",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-unit-jest": "^4.2.3",
    "@vue/cli-service": "^3.8.4",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "firebase": "^7.13.2",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": "8.x"
  }
}
